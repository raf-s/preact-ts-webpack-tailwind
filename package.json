{
  "name": "preact-ts-webpack-tailwind",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Raf Sz.",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "start": "webpack-cli serve --mode=development --env development --open --hot",
    "build": "webpack --mode=production --env production --progress",
    "format:other": "yarn prettier --write \"{,!(generated)/**/}*.{json,gql,graphql,yaml,yml}\"",
    "format:code": "yarn prettier --write \"{,!(generated)/**/}*.{ts,tsx,js,jsx}\"",
    "fix:code": "yarn lint:code --fix",
    "fix": "yarn fix:code && yarn fix:css && yarn format:code && yarn format:other",
    "prettier": "prettier --ignore-path .gitignore",
    "lint:other": "yarn prettier --list-different \"{,!(generated)/**/}*.{json,gql,graphql,yaml,yml}\"",
    "lint:code": "eslint --max-warnings=0 --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint": "yarn lint:other && yarn lint:code && yarn lint:css"
  },
  "lint-staged": {
    "!(generated)/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ],
    "!(generated)/**/*.{json,gql,graphql,yaml,yml}": [
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@types/react-helmet": "^6.1.5",
    "clsx": "^1.1.1",
    "preact": "^10.6.4",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.0",
    "@types/tailwindcss": "^3.0.2",
    "@types/webpack-dev-server": "^4.5.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "autoprefixer": "^10.4.2",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.15",
    "eslint": "^8.5.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.13",
    "terser-webpack-plugin": "^5.3.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
